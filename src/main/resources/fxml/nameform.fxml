<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.krook1024.game.controller.NameFormController">
    <VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0">
        <padding>
            <Insets left="20" right="20" />
        </padding>
      <children>
         <Label text="Name">
            <graphic>
               <TextField fx:id="name" />
            </graphic>
         </Label>
         <Button mnemonicParsing="false" text="Play" />
      </children>
    </VBox>
</AnchorPane>
