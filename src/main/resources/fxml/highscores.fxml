<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.krook1024.game.controller.HighScoreController">
    <children>
      <VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0">
         <children>
              <Label text="High Scores" VBox.vgrow="ALWAYS">
                  <font>
                      <Font size="22.0" />
                  </font>
              </Label>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <children>
                    <TableView fx:id="highScoreTable" editable="true" maxHeight="250.0" maxWidth="1.7976931348623157E308" prefHeight="250.0" HBox.hgrow="ALWAYS">
                        <columns>
                            <TableColumn fx:id="player" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="100.0" text="Player" />
                            <TableColumn fx:id="steps" maxWidth="1.7976931348623157E308" prefWidth="50.0" text="Steps" />
                            <TableColumn fx:id="duration" prefWidth="100.0" text="Duration" />
                            <TableColumn fx:id="created" prefWidth="250.0" text="Date" />
                        </columns>
                    </TableView>
               </children>
            </HBox>
              <Button mnemonicParsing="false" onAction="#onMainMenuButtonClicked" text="Main Menu" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
    </children>
</Pane>
